    "url": "https://github.com/formio/appserver/issues"
  },
  "files": [
    "lib",
    "server.js",
    "package.json",
    "README.md"
  ],
  "homepage": "https://github.com/formio/appserver#readme",
  "dependencies": {
    "@formio/appserver-auth-jwt": "^2.0.0-dev.6",
    "@formio/appserver-core": "^2.0.0-dev.6",
    "@formio/appserver-db-mongodb": "^2.0.0-dev.6",
    "@formio/appserver-types": "^2.0.0-dev.4",
    "@formio/core": "^2.0.0-rc.16",
    "@formio/vm": "^0.0.7-rc.10",
    "async": "^3.2.5",
    "bcryptjs": "^2.4.3",
